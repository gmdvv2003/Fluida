{
	"name": "fluida-new-backend",
	"version": "1.0.0",
	"description": "",
	"main": "./dist/index.js",
	"scripts": {
		"start-dev": "npm run migration:generate && npm run babel:build && npm run babel:run",
		"start-prod": "npm run babel:run",
		"babel:build": "del /q /s .\\dist\\* > nul && babel src -d dist && node ./dist/index.js",
		"babel:run": "node ./dist/index.js",
		"migration:generate": "del .\\src\\migrations\\*.js && typeorm migration:generate ./src/migrations/fluida -d ./src/database/Database.js -o",
		"migration:run": "typeorm migration:run -d ./src/database/Database.js",
		"migration:drop": "typeorm schema:drop -d ./src/database/Database.js",
		"migration:sync": "npm run migration:generate && npm run migration:run"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"firebase-admin": "^12.0.0",
		"handlebars": "^4.7.8",
		"json-body-parser": "^1.0.2",
		"jsonwebtoken": "^9.0.2",
		"mailtrap": "^3.3.0",
		"mysql2": "^3.9.4",
		"nodemailer": "^6.9.13",
		"socket.io": "^4.7.5",
		"typeorm": "^0.3.20",
		"uuid": "^9.0.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.24.1",
		"@babel/core": "^7.24.4",
		"@babel/plugin-proposal-decorators": "^7.24.1",
		"@babel/preset-env": "^7.24.4"
	}
}
